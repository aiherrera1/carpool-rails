<p id="notice"><%= notice %></p>

<p>
  <strong>From user:</strong>
  <%= @review.from_user.name %>
</p>

<p>
  <strong>To user:</strong>
  <%= @review.to_user.name %>
</p>

<p>
  <strong>Stars:</strong>
  <%= @review.stars %>
</p>

<p>
  <strong>Title:</strong>
  <%= @review.title %>
</p>

<p>
  <strong>Comment:</strong>
  <%= @review.comment %>
</p>


<% if @review.from_user.id == current_user.id %>
  <%= link_to 'Edit', edit_review_path(@review) %> |
<% end %>
<%= link_to 'Back', reviews_path %>
